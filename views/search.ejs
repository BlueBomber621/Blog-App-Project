<div class="author">
  <h1 class="author__heading">Hello, I'm Developer.</h1>
  <p class="author__body">Web Developer and Student</p>
</div>

<img src="/img/techtree.png" alt="image" />

<section class="articles">
  <h2 class="articles__heading">Latest Posts</h2>

  <!-- Dummy data is displayed for now, until we include posts -->
  <ul class="article-ul">
    <% data.forEach(post => {%>
    <li>
      <a href="/post/<%= post._id %>">
        <span><%- post.title %></span>
        <span class="article-list__date">
          <% post.createdAt.toDateString() %>
        </span>
      </a>
    </li>
    <%}); %>
  </ul>
</section>
